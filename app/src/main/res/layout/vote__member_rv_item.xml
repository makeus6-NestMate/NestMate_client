<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/vote_rv_item_layout"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginLeft="16dp"
    android:layout_marginRight="16dp"
    android:layout_marginTop="6dp"
    android:layout_marginBottom="6dp"
    android:paddingTop="14dp"
    android:paddingBottom="14dp"
    android:background="@drawable/today_todo_background"
    android:clipToPadding="false"
    android:elevation="2dp">


    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/vote_member_rv_item_img"
        android:layout_width="45dp"
        android:layout_height="45dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:src="@drawable/chicken_img"
        android:layout_marginLeft="12dp"/>

    <TextView
        android:id="@+id/vote_member_rv_item_nickname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/notosanskr_medium"
        android:textStyle="normal"
        android:textSize="16dp"
        android:textColor="@color/black"
        android:lineSpacingExtra="17dp"
        android:includeFontPadding="false"
        android:text="test"
        android:layout_marginLeft="14dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@id/vote_member_rv_item_img"/>



</androidx.constraintlayout.widget.ConstraintLayout>